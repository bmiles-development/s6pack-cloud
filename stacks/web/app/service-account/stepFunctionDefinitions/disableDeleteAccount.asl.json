{
    "StartAt": "RemoveDeleteAccountFlag",
    "States": {
      "RemoveDeleteAccountFlag": {
        "Type": "Task",
        "Resource": "arn:aws:states:::dynamodb:updateItem",
        "Parameters": {
          "TableName": "${dataStackName}-Tenants",
          "Key": {
            "Id": {
              "S.$": "$.identity.claims.name"
            }
          },
          "UpdateExpression": "REMOVE DeleteAccountFlag"
        },
        "ResultPath": "$.success",
        "Next": "ReturnSuccess"
      },
      "ReturnSuccess": {
        "Type": "Pass",
        "End": true,
        "Parameters": {
          "success": "true"
        }
      }
    }  
  }